{
  "name": "agent-portal-demo",
  "version": "1.0.0",
  "description": "The next gen of agent experience by Novo",
  "private": true,
  "keywords": [
    "nextjs",
    "react",
    "design-system",
    "tailwindcss",
    "radix-ui",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "packageManager": "yarn@4.6.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "yarn workspace @novo/portal dev",
    "build": "yarn workspace @novo/portal build",
    "start": "yarn workspace @novo/portal start",
    "lint": "yarn workspace @novo/portal lint",
    "test": "VITEST_PROJECT=unit vitest",
    "test:storybook": "VITEST_PROJECT=storybook vitest",
    "test:all": "vitest",
    "storybook": "yarn workspace @novo/ui storybook",
    "build-storybook": "yarn workspace @novo/ui build-storybook"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.3",
    "@storybook/addon-a11y": "^10.0.8",
    "@storybook/addon-docs": "^10.0.8",
    "@storybook/addon-onboarding": "^10.0.8",
    "@storybook/addon-vitest": "^10.0.8",
    "@storybook/nextjs-vite": "^10.0.8",
    "@vitest/browser-playwright": "^4.0.12",
    "next": "^16.0.7",
    "playwright": "^1.56.1",
    "react": "^19.0.1",
    "react-dom": "^19.0.1",
    "storybook": "^10.0.8",
    "vite": "^5.4.10",
    "vitest": "^4.0.12"
  }
}
